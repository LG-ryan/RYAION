# VMSI-SDM 환경 변수 설정
# 이 파일을 .env로 복사하여 사용하세요

# Database 설정
# 로컬 개발: sqlite:///./vmsi_sdm.db
# 프로덕션: postgresql://user:pass@host:port/dbname
DATABASE_URL=sqlite:///./vmsi_sdm.db

# Server 설정
SERVER_HOST=0.0.0.0
SERVER_PORT=8000

# TradingView Webhook Security (선택사항)
WEBHOOK_SECRET=your_secret_key_here

# Market Data Provider
# yahoo, polygon, binance 등
DATA_PROVIDER=yahoo

# Polygon API (선택사항)
POLYGON_API_KEY=your_polygon_api_key

# Optuna 설정
OPTUNA_TRIALS=100
OPTUNA_TIMEOUT=3600

# Streamlit 설정
STREAMLIT_PORT=8501

# 로깅 레벨
LOG_LEVEL=INFO


